project:
  version: @project.version@
  user:
    baseurl: /
server:
  port: 8080
spring:
  data:
    mongodb:
      database: user
      uri: mongodb://localhost:27017
eureka:
  client:
    service-url: http://localhost:1111/eureka/

# Disable Spring Boot's "Whitelabel" default error page, so we can use our own
error:
  whitelabel:
    enabled: false