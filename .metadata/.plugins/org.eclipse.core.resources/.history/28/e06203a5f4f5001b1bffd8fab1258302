server:
  port: 8081
  error:
    whitelabel:
      enabled: false
spring:
  data:
    mongodb:
      database: subscription
      uri: mongodb://localhost:27017
eureka:
  client:
    service-url: 
      default-zone: http://localhost:1111/eureka/
# Disable Spring Boot's "Whitelabel" default error page, so we can use our own
:
  whitelabel:
    enabled: false